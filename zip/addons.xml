<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<addons>
<addon id="plugin.video.ProjectCypher"
       name="ProjectCypherTV"
       version="1.0.2"
       provider-name="Cypher">
  <requires>
    <import addon="xbmc.python" version="2.1.0"/>
	<import addon="script.module.t0mm0.common" version="2.0.0"/>
    <import addon="script.module.beautifulsoup" version="3.2.1"/>
    <import addon="script.module.simple.downloader" version="0.9.4"/>
    <import addon="script.module.beautifulsoup4" />
    <import addon="script.module.simple.downloader" version="0.9.4"/>
    <import addon="script.module.requests" />
    <import addon="script.module.httplib2" />
    <import addon="script.module.youtube.dl" optional="true"/>
    <import addon="plugin.video.youtube" version="3.0.0"/>
    <import addon="script.module.urlresolver" optional="true"/>
    <import addon="script.module.simplejson" />
    <import addon="script.common.plugin.cache" version="2.5.2"/>
    <import addon="script.module.metahandler" version="2.5.1"/>
  </requires>
  <extension point="xbmc.python.pluginsource" library="default.py">
    <provides>video</provides>
  </extension>
  <extension point="xbmc.addon.metadata">
    <summary>Live SD and HD TV channels</summary>
    <description>General disclaimer: Project Cypher takes no responsibility for the content or reliability of the feeds. Project Cypher [B]Does Not[/B] host any of the feeds or Streams. [B]PLEXUS[/B] addon is required for P2P (Torrent) sports channels. Make sure it is properly configured for your system. Most of the Code from this addon was taken from many other addons but used under the GNU GENERAL PUBLIC LICENSE Version 2, June 1991</description>
    <platform>all</platform>
  </extension>
</addon>

<addon id="repository.targetin1080p" name="targetin1080p Repo" version="1.1.0001" provider-name="targetin1080p">
	<extension point="xbmc.addon.repository" name="Targetin1080p Repository">
		<info compressed="false">https://raw.githubusercontent.com/targetin1080p/targetin1080p/master/zip/addons.xml</info>
		<checksum>https://raw.githubusercontent.com/targetin1080p/targetin1080p/master/zip/addons.xml.md5</checksum>
		<datadir zip="true">https://github.com/targetin1080p/targetin1080p/raw/master/zip/</datadir>
	</extension>
	<extension point="xbmc.addon.metadata">
		<summary>The Targetin1080p Repo</summary>
		<description>Download and install addons from Targetin1080p repo</description>
		<platform>all</platform>
	</extension>
</addon>
</addons>
